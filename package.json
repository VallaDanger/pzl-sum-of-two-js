{
  "name": "sum-of-two-js",
  "version": "1.0.0",
  "description": "pzl-sum-of-two-js",
  "scripts": {
    "test": "mocha --recursive",
    "coverage": "nyc mocha --recursive --reporter mocha-multi-reporters --reporter-options configFile=.mocha-multi-config.json -r ts-node/register test/**/*.test.ts && nyc report",
    "sonar": "npm run coverage && sonar-scanner",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc --declaration",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "prepublish": "npm run compile",
    "start": "ts-node ./src/app.ts"
  },
  "directories": {
    "src": "./src",
    "test": "./test"
  },
  "keywords": [
    "js",
    "puzzles",
    "compsci"
  ],
  "author": "chux",
  "license": "Apache-2.0",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.21",
    "@types/mocha": "latest",
    "@types/node": "^14.11.2",
    "assert": "^2.0.0",
    "chai": "latest",
    "gts": "^3.1.0",
    "mocha": "latest",
    "mocha-multi-reporters": "^1.5.1",
    "mocha-sonarqube-reporter": "^1.0.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.2.1",
    "tslint": "^6.1.3",
    "typescript": "^4.4.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
